<div class="header-cont">
    <h2>Ironhack Cinema</h2>
    <p>Click on the movie to check the showtimes!</p>
</div>
<div class="movies-cont">
{{#each movies as |movie|}}
<div class="content-cont">
<img src= {{movie.image}}> 
 <h1> {{movie.title}}</h1> 
<a href="movies/{{movie._id}}">See more</a>
</div>
{{/each}}
</div>


{{#if err}}
<h1>{{err}}</h1>
{{/if}}
